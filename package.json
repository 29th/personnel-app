{
  "name": "personnel",
  "version": "0.0.0",
  "description": "Personnel System ================",
  "main": "app/index.html",
  "scripts": {
    "install": "napa",
    "build": "browserify app/scripts/shims/marionette.js app/scripts/main.js -o app/scripts/build.js -d",
    "watch": "watchify app/scripts/shims/marionette.js app/scripts/main.js -o app/scripts/build.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/29th/personnel-app.git"
  },
  "author": "29th Infantry Division",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/29th/personnel/issues"
  },
  "dependencies": {
    "jquery": "~2.1.3",
    "underscore": "~1.7.0",
    "backbone": "~1.1.2",
    "backbone.marionette": "~1.6.4",
    "hbsfy": "~2.2.1",
    "handlebars": "~3.0.0",
    "moment": "~2.9.0",
    "bootstrap": "~3.3.2",
    "fullcalendar-browser": "~1.5.4-1",
    "nprogress": "~0.1.6",
    "bbcode": "~0.1.5",
    "bootstrap-datepicker": "git://github.com/eternicode/bootstrap-datepicker",
    "backbone.validation": "~0.7.1",
    "bootstrap-select": "~1.6.3",
    "moment-duration-format": "~1.3.0",
    "moment-timezone": "~0.3.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "event-stream": "~3.1.0",
    "gulp": "~3.5.2",
    "gulp-beautify": "~1.0.3",
    "gulp-concat": "~2.1.7",
    "gulp-minify-css": "~0.3.0",
    "gulp-minify-html": "~0.1.0",
    "gulp-processhtml": "0.0.1",
    "gulp-requirejs": "~0.1.3",
    "gulp-uglify": "~0.2.1",
    "gulp-util": "~2.2.14",
    "gulp-wrap-umd": "~0.1.0",
    "del": "~1.1.1",
    "napa": "~1.2.0",
    "browserify-shim": "~3.8.2"
  },
  "napa": {
    "jquery-nestable": "dbushell/Nestable"
  },
  "browserify": {
    "transform": [
      [
        "hbsfy",
        {
          "extensions": [
            "html"
          ]
        }
      ],
      "browserify-shim"
    ]
  },
  "browser": {
    "bootstrap-datepicker": "bootstrap-datepicker/dist/js/bootstrap-datepicker",
    "bootstrap-select": "bootstrap-select/dist/js/bootstrap-select",
    "jquery-nestable": "jquery-nestable/jquery.nestable",
    "fullcalendar-browser": "fullcalendar-browser/fullcalendar/fullcalendar"
  },
  "browserify-shim": {
    "jquery": "global:jQuery"
  }
}
